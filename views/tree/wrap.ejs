<!DOCTYPE html>
<html lang="en-US" dir="ltr">

  <head>
    <%- include('../includes/head.ejs') %>
  </head>


  <body>

    <!-- ===============================================-->
    <!--    Main Content-->
    <!-- ===============================================-->
    <main class="main" id="top">
        <%- include('../includes/menu.ejs') %>
      
        <%- include('../includes/top.ejs') %>
      <div class="content">
        <%- include('../includes/nav.ejs') %>

      
        
        <div id="members" >
          <div class="mb-4">
            <div class="d-flex flex-col justify-content-between gap-4">
              <div class="col-1 ">
                <h2 class="text-bold text-1100 mb-5"><%=basicInfo.title%></h2>
              </div>
              <div class="search-box">
                <form class="position-relative" data-bs-toggle="search" data-bs-display="static">
                  <input class="form-control search-input search" id="srchText" type="search" placeholder="선택 조회" aria-label="Search" value="<%=memId%>"/>
                  <span class="fas fa-search search-box-icon"></span>
                </form>
              </div>
              <div class="col-1">
                <button class="btn btn-outline-primary me-1 mb-1" type="button" onClick="fnSearch()" >검색</button>
              </div>
              <div class="ms-xxl-auto">
              </div>
            </div>
          </div>
          <div class="mx-n4 mx-lg-n6 px-4 px-lg-6 mb-9 bg-white border-y border-300 mt-2 position-relative top-1">
            <iframe src="<%=link%>?memId=<%=memId%>" style="width:100%; height:800px" class="-mt-2"  id="binaryTree" name="binaryTree"></iframe>
          </div>
        </div>
        <!-- footer -->
        <%- include('../includes/footer.ejs') %>
      </div>
   
    </main>
    <!-- ===============================================-->
    <!--    End of Main Content-->
    <!-- ===============================================-->


   
    <!-- ===============================================-->
    <!--    JavaScripts-->
    <!-- ===============================================-->
    <%- include('../includes/scripts.ejs') %>
    <script src="https://code.jquery.com/jquery-3.6.3.js"></script>
   <%- include('../includes/modal.ejs') %>
    <script>
    function fnSearch() {
      
      let pageIndex = $('#pageIndex').val();
      let rowsPerPage = 10 //$('#rowsPerPage').val();
      let memId = $('#srchText').val();

      let form = document.createElement("form");
      form.setAttribute("charset", "UTF-8");
      form.setAttribute("method", "Post");
      form.setAttribute("action", "/ad/o/wrap");

      let hiddenField1 = document.createElement("input");
      hiddenField1.setAttribute("type", "hidden");
      hiddenField1.setAttribute("name", "memId");
      hiddenField1.setAttribute("value", memId);
      form.appendChild(hiddenField1);

      document.body.appendChild(form);
      form.submit();
    }
    
    function childSearch(memId) {
      $('#srchText').val(memId);
      fnSearch()
    }
    </script>
  </body>

</html>